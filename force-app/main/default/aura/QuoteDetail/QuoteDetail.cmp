                    <td style="text-align:left body; font-size:12px; width:50%;"><b>Status :</b></td>
                    <td style="font-size:12px; width:50%;">&nbsp;&nbsp;{!v.dealModelWrapper.data.ES_MARGIN_HDR.STATUS}</td>
                </tr>
            </table> 
        </lightning:layoutItem>
    </lightning:layout>
