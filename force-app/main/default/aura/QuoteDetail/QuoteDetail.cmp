<aura:component >
	<aura:attribute name="dealModelWrapper" type="Object" /> 
    <lightning:layout multipleRows="true">
        <lightning:layoutItem flexibility="auto" padding="around-small" size="10" smallDeviceSize="10" mediumDeviceSize="4" largeDeviceSize="4" >
            <table style="width:100%;">  
                <tr>      
                    <td style="text-align:left body; font-size:12px; width:50%;"><b>Transaction No:</b></td>
                    <td style="font-size:12px; width:50%;">&nbsp;&nbsp;<ui:outputNumber value="{!v.dealModelWrapper.data.ES_MARGIN_HDR.ORDER_NUM}" format="#############"/></td>
                </tr>
            </table>
        </lightning:layoutItem>
        <lightning:layoutItem flexibility="auto" padding="around-small" size="10" smallDeviceSize="10" mediumDeviceSize="4" largeDeviceSize="4" >
            <table style="width:100%;">  
                <tr>      
                    <td style="text-align:left body; font-size:12px; width:50%;"><b>Transaction Type:</b></td>
                    <td style="font-size:12px; width:50%;">&nbsp;&nbsp;{!v.dealModelWrapper.data.ES_MARGIN_HDR.ORDER_TYPE_DESC}</td>
                </tr>
            </table>
        </lightning:layoutItem>
        <lightning:layoutItem flexibility="auto" padding="around-small" size="10" smallDeviceSize="10" mediumDeviceSize="4" largeDeviceSize="4s" >
            <table style="width:100%;">  
                <tr>      
                    <td style="text-align:left body; font-size:12px; width:50%;"><b>Description:</b></td>
