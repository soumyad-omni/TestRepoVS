    public static Map<String,String> calloutPandoraAuth()
    {
        Pandora_Endpoint_Details__c pandoraDetails = Pandora_Endpoint_Details__c.getInstance('Pandora');
        
        httpRequest req =  new httpRequest();
        req.setendpoint('https://login.salesforce.com/services/oauth2/token');
        req.setMethod('POST');
        
        req.setBody('grant_type=password'
                    +'&client_id='+pandoraDetails.Client_Id__c
                    +'&client_secret='+pandoraDetails.Client_Secret__c
                    +'&username='+pandoraDetails.UserName__c
                    +'&password='+pandoraDetails.Password__c);
        httpResponse res = new http().send(req);
        system.debug('access res-->'+res);
        system.debug('access res body-->'+res.getBody());
        Map<String,String> resMap =  (Map<String,String>)JSON.deserialize(res.getBody(),Map<String,String>.class);
        return resMap;
    }
    
    public static String calloutPandoraAPI(String token, String endPoint)
    {
        
        httpRequest req =  new httpRequest();
        req.setendpoint(endPoint);
        req.setMethod('GET');
        req.setHeader('Content-Type','application/json');
        req.setHeader('Authorization','Bearer '+token);
        
        httpResponse res = new http().send(req);
        system.debug('res-->'+res);
        system.debug('res body-->'+res.getBody());
        
        return res.getBody();
    }
