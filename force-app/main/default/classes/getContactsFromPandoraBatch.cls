    global void execute(Database.BatchableContext BC, List<Account> accList)
    {
        Organization org = [Select Id, Name, IsSandbox from Organization];
        
        String csnList = '(';
        
        for(Account acc : accList)
        {
            csnList += ''+acc.SAP_Customer_Number__c+',';
        }
        csnList = csnList.removeEnd(',');
        csnList += ')';
        system.debug('csnList-->'+csnList);

        Map<String,String> resMap = calloutPandoraAuth();
        
        String endPointPath = resMap.get('instance_url')+'/services/data/v60.0/query/?q=select+id,name,firstname,lastname,phone,email,Account.CSN__C+from+contact+where+Account.CSN__C+IN+'+csnList;
        
        Map<String, List<Records>> pandoraMap = new Map<String, List<Records>>();
		Boolean hasMoreRecords = false;        
        String nextBatchPath = '';
        do
        {
            String jsonResponse;
            if(!hasMoreRecords)
            {
                jsonResponse = calloutPandoraAPI(resMap.get('access_token'),endPointPath);
            }
            else
            {
                jsonResponse = calloutPandoraAPI(resMap.get('access_token'), resMap.get('instance_url')+''+nextBatchPath);
            }
            jsonResponse = jsonResponse.remove('__c');
            contactWrapper cls = (contactWrapper) JSON.deserialize(jsonResponse,contactWrapper.Class);
            //system.debug('cls-->'+cls.nextRecordsUrl);
            
            if(cls.nextRecordsUrl <> NULL)
            {
                nextBatchPath = cls.nextRecordsUrl;
                hasMoreRecords = true;
            }
            else
            {
                nextBatchPath = '';
                hasMoreRecords = false;
            }
            
            for (Records rec : cls.records)
            {
                if(pandoraMap.containsKey(String.valueOf(Integer.valueOf(rec.Account.CSN))))
                {
                    List<Records> temp = pandoraMap.get(String.valueOf(Integer.valueOf(rec.Account.CSN)));
                    temp.add(rec);
                    pandoraMap.put(String.valueOf(Integer.valueOf(rec.Account.CSN)),temp);
                }
                else
                {
                    List<Records> temp = New List<Records>();
                    temp.add(rec);
                    pandoraMap.put(String.valueOf(Integer.valueOf(rec.Account.CSN)),temp);
                }
            }
        }
        while(hasMoreRecords);
        
        
        List<Contact> insertContacts = new List<Contact>();
        for(Account acc : accList)
        {
            if(pandoraMap.containsKey(acc.SAP_Customer_Number__c))
            {
                for(Records rec : pandoraMap.get(acc.SAP_Customer_Number__c))
                {
                    Boolean foundContact = false;
                    
                    for(Contact con : acc.contacts)
                    {
                        if(con.Name == rec.Name
                           || con.Email == rec.Email
                           || con.Email == rec.Email+'.invalid'
                           || con.Other_Email_Address__c == rec.Email
                           || con.Other_Email_Address__c == rec.Email+'.invalid'
                           || con.Phone == rec.Phone
                          )
                        {
                            foundContact = true;
                            break;
                        }
                    }

                    if(!foundContact)
                    {
                        Contact newContact = New Contact();
                        newContact.AccountId = acc.Id;
                        newContact.FirstName = rec.FirstName;
                        newContact.LastName = rec.LastName;
                        newContact.Email = rec.Email;
                        newContact.Phone = rec.Phone;
                        newContact.Legacy_Pandora_Contact_Id__c = rec.Id;
                        
                        if(org.IsSandbox && newContact.Email <> NULL)
                        {
                            newContact.Email = newContact.Email + '.invalid';
                        }
                        insertContacts.add(newContact);
                    }
                }
            }
        }
        
        system.debug('insertContacts-->'+insertContacts.size()+':::'+insertContacts);
        if(!insertContacts.isEmpty())
        {
            insert insertContacts;
        }
    }
    
