                <td width="15%">
                    <apex:outputText value="{0, Number, ###,###,##0.00}">
                        <apex:param value="{!Deal_Modeling_PDF__c.Customer_Value_Price__c}" />
                    </apex:outputText>
                </td>
                <td width="15%">Total Margin:</td>
                <td width="15%"> 
                    <apex:outputText value="{0, Number, ###,###,##0.00}">
                        <apex:param value="{!Deal_Modeling_PDF__c.Total_Margin__c}" />
                    </apex:outputText>
                </td>
